{
  "name": "relational-db-testing",
  "version": "1.2.0",
  "description": "A comprehensive package for testing relational databases with both Node.js and Python implementations",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yashwant-das/relational-db-testing.git"
  },
  "scripts": {
    "test": "jest",
    "test:connection": "node test-connection.js",
    "test:all": "./setup-and-test.sh",
    "setup": "npm install && cd python && pip install -r requirements.txt",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:logs": "docker-compose logs mysql"
  },
  "keywords": ["mysql", "database", "testing", "jest", "nodejs", "python", "docker", "crud"],
  "author": "Yashwant Das",
  "license": "ISC",
  "devDependencies": {
    "jest": "^30.0.4"
  },
  "dependencies": {
    "@faker-js/faker": "^9.5.0",
    "dotenv": "^17.2.0",
    "mysql2": "^3.14.2"
  }
}
